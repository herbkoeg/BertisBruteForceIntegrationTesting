<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<TestCase>
    <description>This testcase demonstrates how you can run only a reset action</description>
    <resetAction>
        <description>define your reset commands here</description>
        <rollBackOnError>true</rollBackOnError>
        <sqlCommands>
            <sqlCommand>insert into xyz ...</sqlCommand>
            <sqlCommand>update into xyz ...</sqlCommand>
            <sqlCommand>delete into xyz ...</sqlCommand>
        </sqlCommands>
    </resetAction>
</TestCase>
